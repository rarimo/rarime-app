  "author": "Zero Block Global Foundation",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "rarimo",
    "rarime",
    "proof-of-humanity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/rarime-app.git"
